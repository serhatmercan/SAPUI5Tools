<mvc:View
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:mvc="sap.ui.core.mvc"
    controllerName="xxx.controller.NotFound"
>
    <IllustratedMessage illustrationType="sapIllus-PageNotFound">
        <additionalContent>
            <Button
                text="{i18n>goBack}"
                press="onNavBack"
            />
        </additionalContent>
    </IllustratedMessage>
</mvc:View>
