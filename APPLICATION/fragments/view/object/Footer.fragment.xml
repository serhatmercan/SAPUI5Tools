<core:FragmentDefinition xmlns="sap.m"
    xmlns:core="sap.ui.core">
    <OverflowToolbar>
        <Button id="MessagePO" icon="sap-icon://alert" text="{=${message>/}.length}" type="Emphasized" visible="{=${message>/}.length > 0}" press="onShowMessages" />
        <ToolbarSpacer />
        <Button icon="sap-icon://save" text="{i18n>save}" type="Emphasized" visible="{= !${model>/Status} &amp;&amp; ${model>/Edit}}" press="onProcess('SV', $event)" />
    </OverflowToolbar>
</core:FragmentDefinition>
