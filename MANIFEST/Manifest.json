{
	"sap.app": {
		"dataSources": {
			"mainService": {
				"uri": "/sap/opu/odata/sap/ZSM_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml",
					"annotations": [
						"annotation"
					]
				}
			}
		}
	},
	"sap.ui": {
		"fullWidth": true
	},
	"sap.ui5": {
		"dependencies": {
			"minUI5Version": "1.71.11",
			"libs": {
				"sap.f": {},
				"sap.gantt": {},
				"sap.m": {},
				"sap.collaboration": {
					"lazy": true
				},
				"sap.ui.comp": {},
				"sap.ui.core": {},
				"sap.ui.layout": {},
				"sap.ui.table": {},
				"sap.ui.unified": {},
				"sap.ushell": {},
				"sap.uxap": {}
			}
		},
		"flexEnabled": false,
		"handleValidation": true,
		"models": {
			"": {
				"dataSource": "mainService",
				"preload": true,
				"settings": {
					"defaultBindingMode": "TwoWay",
					"defaultCountMode": "Inline"
				}
			},
			"viewModel": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {},
				"uri": "./model/viewModel.json",
				"preload": true
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		}
	}
}