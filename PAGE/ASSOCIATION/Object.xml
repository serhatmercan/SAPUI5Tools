<mvc:View xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:semantic="sap.m.semantic" controllerName="com.serhatmercan.controller.Object">
	<semantic:FullscreenPage id="idObjectPage">
		<semantic:content>
			<Table id="idObjectTable" items="{NavigationPropertiesName}">
				<columns>
					<Column>
						<Text text="{i18n>objectId}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem>
						<cells>
							<Text text="{ObjectId}"/>
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</semantic:content>
	</semantic:FullscreenPage>
</mvc:View>