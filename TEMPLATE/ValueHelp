<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:smartTable="sap.ui.comp.smarttable">
	<Dialog title="{i18n>InOut}" afterClose="onACInOut">
		<content>
			<smartTable:SmartTable id="InOutTable" entitySet="InOutSet" tableBindingPath="InOuts"
				enableAutoBinding="true" header="{i18n>InOut}"
				initiallyVisibleFields="MinAdet,MinAgirlik,MinEn,MinBoy,MinYuks" requestAtLeastFields="MinAdet" showRowCount="true"
				showTablePersonalisation="false" tableType="ResponsiveTable" useExportToExcel="false"/>
		</content>
		<endButton>
			<Button text="{i18n>close}" type="Emphasized" press="onACInOut"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>

onACInOut: function () {
			this._oInOutDialog.close();
			this._oInOutDialog.destroy();
			this._oInOutDialog = null;
		},
		
		onShowInOut: function () {
			this._oInOutDialog = sap.ui.xmlfragment("InOutDialog",
				"com.tofas.mm.partquerycockpit.fragment.InOut",
				this);
			sap.ui.core.Fragment.byId("InOutDialog", "InOutTable").rebindTable();
			this.getView().addDependent(this._oInOutDialog);
			this._oInOutDialog.open();
		},
		
<Annotations Target="Metadata.InOut">
				<Annotation Term="UI.HeaderInfo">
					<Record Type="UI.HeaderInfoType">
						<PropertyValue Property="TypeName" String="{@i18n&gt;InOut}"/>
						<PropertyValue Property="TypeNamePlural" String="{@i18n&gt;InOuts}"/>
					</Record>
				</Annotation>
				<Annotation Term="UI.LineItem">
					<Collection>
						<Record Type="UI.DataField">
							<PropertyValue Property="Value" Path="MinAdet"/>
						</Record>
					</Collection>
				</Annotation>
			</Annotations>


