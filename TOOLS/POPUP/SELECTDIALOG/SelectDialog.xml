	<SelectDialog id="idSD" title="Select Dialog - Title" items="{/SelectDialogSet}" contentHeight="auto" confirmButtonText="{i18n>confirm}"
		confirm="onConfirmSD" cancel="onCancelSD" search="onSearchSD" multiSelect="true">
