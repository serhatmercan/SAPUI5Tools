<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<Dialog id="idDialog" title="Dialog Title" afterClose="onACDialog">
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<Title text="{i18n>title}"/>
				<ToolbarSpacer/>
				<Button press="onPress"/>
			</Toolbar>
		</customHeader>
		<content>
			<Label text="{viewModel>Value/Value1}"/>
		</content>
		<!--	
		<buttons>
			<Button text="{i18n>button1}" press="onPressButton1"/>
			<Button text="{i18n>button2}" press="onPressButton2"/>
			<Button text="{i18n>button3}" press="onPressButton3"/>
		</buttons>
		-->
		<!--		
		<Toolbar>
			<Button visible="{=${messageModel>/}.length > 0}" text="{= ${messageModel>/}.length }" type="Emphasized" icon="sap-icon://message-popup"
				press="onShowMessages"/>
			<ToolbarSpacer/>
			<Button text="{i18n>button2}" press="onPressButton2"/>
			<Button text="{i18n>button3}" press="onPressButton3"/>
		</Toolbar>
		-->
		<beginButton>
			<Button text="{i18n>continue}" press="onPressContinue"/>
		</beginButton>
		<endButton>
			<Button text="{i18n>cancel}" press="onACDialog"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>
