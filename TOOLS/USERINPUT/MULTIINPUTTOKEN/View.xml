<mvc:View xmlns="sap.m">
	<MultiInput id="idMI" editable="true" enableMultiLineMode="true" maxTokens="5" change="onChange" tokenChange="onTokenChange"
		tokenUpdate="onTokenUpdate" valueHelpRequest="onValueHelpRequest" tokens="{ path:'model>/List', templateShareable:true }">
		<tokens>
			<Token key="{model>ID}" text="{model>Value}"/>
		</tokens>
	</MultiInput>
</mvc:View>
