	<MultiComboBox id="idMCB" items="{model>Items}" selectedKeys="{viewModel>Values}" selectionFinish="onSelectionFinishMCB"
		showSecondaryValues="true">
		<core:ListItem key="{model>ID}" text="{model>Text}" additionalText="{FasonAdi}"/>
	</MultiComboBox>
</core:FragmentDefinition>
