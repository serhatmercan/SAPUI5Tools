<mvc:View xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc">
	<Page>
		<content>
			<Panel>
				<FlexBox height="100px" alignItems="Center" justifyContent="Center">
					<Text class="maintxt" text="{i18n>title}"/>
				</FlexBox>
			</Panel>
			<Panel>
				<FlexBox height="100%" width="100%" alignContent="Center" alignItems="Center" justifyContent="Center">
					<items>
						<GenericTile id="GTI" class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="{i18n>GTI}" press="onGoToObject">
							<TileContent>
								<ImageContent src="sap-icon://add-activity"/>
							</TileContent>
						</GenericTile>
						<GenericTile id="GTII" class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="{i18n>GTII}" press="onGoToObject">
							<TileContent>
								<ImageContent src="sap-icon://add-activity"/>
							</TileContent>
						</GenericTile>
						<GenericTile id="GTIII" class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="{i18n>GTIII}" press="onGoToObject">
							<TileContent>
								<ImageContent src="sap-icon://show-edit"/>
							</TileContent>
						</GenericTile>
						<GenericTile id="GTIV" class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="{i18n>GTIV}">
							<TileContent>
								<NumericContent value="{model>/Value}" valueColor="Good"/>
							</TileContent>
						</GenericTile>
					</items>
				</FlexBox>
			</Panel>
		</content>
	</Page>
</mvc:View>