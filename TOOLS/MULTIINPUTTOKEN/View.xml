<mvc:View xmlns="sap.m" xmln:mvc="sap.mvc">
	<MultiInput tokens="{model>/List}">
		<tokens>
			<Token key="{model>ID}" text="{model>Value}"/>
		</tokens>
	</MultiInput>
</mvc:View>
