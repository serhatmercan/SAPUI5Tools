<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
    <TableSelectDialog items="{model>/Items}" cancel="onCancelTSD" confirm="onConfirmTSD" search="onSearchTSD">
		<columns>
			<Column>
				<Text text="{i18n>Key}"/>
			</Column>
			<Column>
				<Text text="{i18n>Text}"/>
			</Column>
		</columns>
		<ColumnListItem vAlign="Middle">
			<cells>
				<Text text="{model>Key}"/>
				<Text text="{model>Text}"/>
			</cells>
		</ColumnListItem>
	</TableSelectDialog>
</core:FragmentDefinition>
