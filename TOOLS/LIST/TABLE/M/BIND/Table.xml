<Table id="idTable" items="{ path: '/TableSet', sorter: { path: 'ID', descending: false } }">
	<headerToolbar>
		<Toolbar>
			<Title id="idToolbarTitle" text="Toolbar Title"/>
		</Toolbar>
	</headerToolbar>
	<columns>
		<Column>
			<Text text="Name"/>
		</Column>
	</columns>
	<items>
		<ColumnListItem>
			<cells>
				<ObjectIdentifier title="{ID}" text="{Name}"/>
			</cells>
		</ColumnListItem>
	</items>
</Table>

