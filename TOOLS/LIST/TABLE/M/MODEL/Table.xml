<Table id="idTable" items="{tableModel>/}">
	<headerToolbar>
		<Toolbar>
			<ToolbarSeparator/>
		</Toolbar>
	</headerToolbar>
	<infoToolbar>
		<Toolbar>
			<ToolbarSeparator/>
		</Toolbar>
	</infoToolbar>
	<columns>
		<Column mergeDuplicates="true">
			<Text text="{i18n>value1}"/>
		</Column>
		<Column mergeDuplicates="true">
			<Text text="{i18n>value2}"/>
		</Column>
	</columns>
	<items>
		<ColumnListItem id="idCLI">
			<cells>
				<Text text="{tableModel>Value1}"/>
				<Text text="{tableModel>Value2}"/>
			</cells>
		</ColumnListItem>
	</items>
</Table>
