width="10rem"

hAlign="Begin" hAlign="Center" hAlign="Left"
minScreenWidth="Tablet"
popinDisplay="Inline" popinDisplay="Block" 

demandPopin="true"
fixedLayout="false"

select="onSelectTable"

COLUMN LIST ITEM - ATTRIBUTES

