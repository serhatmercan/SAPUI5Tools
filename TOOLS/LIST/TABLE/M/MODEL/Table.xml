backgroundDesign="Transparent"
sticky="ColumnHeaders"

noDataText="{model>/tableNoDataText}"
busyIndicatorDelay="{model>/tableBusyDelay}"

items="{ path: '/TableSet', suspended:true }"

fixedLayout="true"

