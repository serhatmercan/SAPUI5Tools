{
	"sap.ui5": {
		"models": {
			"viewModel": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {},
				"preload": true,
				"uri": "model/viewModel.json"
			}
		},
		"extends": {
			"extensions": {
				"sap.ui.controllerExtensions": {
					"sap.suite.ui.generic.template.ListReport.view.ListReport": {
						"controllerName": "com.sun.serhatmercan.ext.controller.ListReportExt",
						"sap.ui.generic.app": {
							"SummarySet": {
								"EntitySet": "MainSet",
								"Actions": {
									"DetailButton": {
										"id": "idDetailButton",
										"text": "{@i18n>detail}",
										"press": "onGoToDetail"
									}
								}
							}
						}
					}
				},
				"sap.ui.viewExtensions": {
					"sap.suite.ui.generic.template.ListReport.view.ListReport": {
						"GridTableColumnsExtension|MainSet": {
							"type": "XML",
							"className": "sap.ui.core.Fragment",
							"fragmentName": "com.sun.serhatmercan.ext.view.ListReport"
						}
					},
					"sap.suite.ui.generic.template.ObjectPage.view.Details": {
						"GridTableColumnsExtension|DetailSet|Items::com.sap.vocabularies.UI.v1.LineItem": {
							"className": "sap.ui.core.Fragment",
							"fragmentName": "com.sun.serhatmercan.ext.view.DetailExt",
							"type": "XML"
						}
					}
				}
			}
		}
	},
	"sap.ui.generic.app": {
		"_version": "1.3.0",
		"pages": {
			"ListReport|MainSet": {
				"entitySet": "MainSet",
				"component": {
					"name": "sap.suite.ui.generic.template.ListReport",
					"list": true,
					"settings": {
						"tableSettings": {
							"type": "GridTable",
							"multiSelect": true,
							"selectAll": true,
							"smartVariantManagement": true
						}
					}
				},
				"pages": {
					"ObjectPage|MainSet": {
						"entitySet": "MainSet",
						"component": {
							"name": "sap.suite.ui.generic.template.ObjectPage",
							"settings": {
								"sections": {
									"Items::com.sap.vocabularies.UI.v1.LineItem": {
										"navigationProperty": "Items",
										"entitySet": "Detail",
										"tableSettings": {}
									}
								}
							}
						}
					}
				}
			}
		}
	}
}